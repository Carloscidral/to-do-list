<!-- StickyStudy Logo - Yellow Post-it Style 512x512px -->
<svg xmlns="http://www.w3.org/2000/svg" width="512" height="512" viewBox="0 0 512 512">
  <!-- 3D Shadow Base -->
  <rect x="45" y="50" width="380" height="380" fill="rgba(0,0,0,0.15)" rx="25" transform="translate(8,8)"/>
  
  <!-- Main Post-it Note -->
  <rect x="45" y="50" width="380" height="380" fill="#FFE066" rx="25" stroke="#E6CC00" stroke-width="3"/>
  
  <!-- Inner highlight for 3D effect -->
  <rect x="48" y="53" width="374" height="374" fill="none" stroke="rgba(255,255,255,0.4)" stroke-width="2" rx="22"/>
  
  <!-- Corner fold effect -->
  <path d="M370 75 L370 120 L325 120 Z" fill="#E6CC00" stroke="#CCAA00" stroke-width="1"/>
  <path d="M370 75 L370 120 L325 120 Z" fill="rgba(255,255,255,0.2)"/>
  
  <!-- Sticky tape effect at top -->
  <rect x="200" y="25" width="80" height="25" fill="rgba(255,255,255,0.3)" rx="8"/>
  <rect x="205" y="30" width="70" height="15" fill="rgba(255,255,255,0.2)" rx="5"/>
  
  <!-- Text: StickyStudy -->
  <text x="235" y="200" text-anchor="middle" font-family="Arial Black, Arial, sans-serif" font-size="48" font-weight="900" fill="#1A365D">Sticky</text>
  <text x="235" y="260" text-anchor="middle" font-family="Arial Black, Arial, sans-serif" font-size="48" font-weight="900" fill="#1A365D">Study</text>
  
  <!-- Decorative underline -->
  <line x1="120" y1="280" x2="350" y2="280" stroke="#E6CC00" stroke-width="6" stroke-linecap="round"/>
  
  <!-- Small decorative elements -->
  <circle cx="140" cy="320" r="4" fill="#E6CC00"/>
  <circle cx="235" cy="320" r="4" fill="#E6CC00"/>
  <circle cx="330" cy="320" r="4" fill="#E6CC00"/>
  
  <!-- Subtle ruled lines like real post-it -->
  <line x1="80" y1="350" x2="390" y2="350" stroke="rgba(230,204,0,0.3)" stroke-width="1"/>
  <line x1="80" y1="370" x2="390" y2="370" stroke="rgba(230,204,0,0.3)" stroke-width="1"/>
  <line x1="80" y1="390" x2="390" y2="390" stroke="rgba(230,204,0,0.3)" stroke-width="1"/>
</svg>